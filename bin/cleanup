#!/bin/sh

sudo umount output/system
sudo umount output/vendor
sudo umount output/odm
sudo umount output/product

rm -rf output
rm -rf tmp

rm -f bin/simg2img
rm -f bin/img2simg
rm -f bin/aml_image_extractor
rm -f bin/abootimg
rm -rf bin/lpadd
rm -rf bin/lplpdump
rm -rf bin/lpflash
rm -rf bin/lpmake
rm -rf bin/lpunpack

make -C bin/src/simg2img/ clean
make -C bin/src/abootimg/ clean

echo "Cleanup done"

cc_defaults {
    name: "protobuf-cflags-defaults",
    clang_cflags: [
        "-Wno-unused-const-variable",
        "-Wno-error=user-defined-warnings",
    ],
    cflags: [
        "-Wall",
        "-Werror",
        "-Wno-unused-function",
        "-Wno-sign-compare",
        "-Wno-unused-parameter",
        "-Wno-sign-promo",
    ],

    target: {
        windows: {
            cflags: ["-Wno-macro-redefined"],
        },
    },
}

